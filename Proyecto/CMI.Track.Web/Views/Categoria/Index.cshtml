
@{
    ViewBag.Title = "Categorias";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            <small>Lista</small> Categorias
            <button class="btn btn-outline btn-primary pull-right btnNuevo" style="display: none;">Nuevo</button>
        </h1>        
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <div class="col-lg-12">
         <div id="bbGrid-clear" class="col-lg-12">  </div>                   
    </div>
</div>

<div class="modal fade bs-example-modal-lg" id="nuevo-categoria"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoCategoriaLabel">
</div>

<div class="modal fade bs-example-modal-lg" id="actualiza-categoria"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoCategoriaLabel">
</div>


@section Scripts {
    <script src="~/Scripts/Categoria.js"></script>
}

