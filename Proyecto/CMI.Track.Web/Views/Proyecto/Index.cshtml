@{
    ViewBag.Title = "Proyectos ";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            <small>Lista</small> Proyectos
            <button class="btn btn-outline btn-primary pull-right btnNuevo" style="display: none;">Nuevo</button>
        </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <div class="col-lg-12">
        <div id="bbGrid-clear" class="col-lg-12">
            <div id="cargandoInfo">Cargando informacion...</div>
        </div>
    </div>
</div>

<div class="modal fade bs-example-modal-lg" id="nuevo-Proyecto"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoProyectoLabel">
</div>

<div class="modal fade bs-example-modal-lg" id="actualiza-Proyecto"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoProyectoLabel">
</div>



@section Scripts {
    <script src="~/Scripts/Proyecto.js"></script>
}



