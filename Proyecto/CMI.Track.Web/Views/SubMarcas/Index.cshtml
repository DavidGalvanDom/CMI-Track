@{
    ViewBag.Title = "SubMarcas";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            <small>Lista</small> SubMarcas
            <button class="btn btn-outline btn-primary pull-right btnNuevo" style="display: none;">Nuevo</button>
        </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <div class="form-group">
        <div class="col-sm-1"> <label class="control-label">Proyecto</label></div>
        <div class="col-sm-4">
            <div class="input-group">
                <p id="nombreProyecto" name="nombreProyecto" class="form-control" placeholder="Nombre Proyecto" title="Nombre Proyecto"> Nombre Proyecto </p>
                <span class="input-group-btn">
                    <button class="btn btn-secondary" type="button" id="btnBuscarProyecto" name="btnBuscarProyecto"><span class="fa fa-search"></span></button>
                </span>
            </div>
            <input type="hidden" id="idProyectoSelect" name="idProyectoSelect" />
        </div>
        <div class="col-sm-2">
            <span id="FechaInicio" placeholder="FechaInicio" class="form-control" title="Fecha Inicio">Fecha Inicio</span>
        </div>
        <div class="col-sm-2">
            <span id="FechaFin" placeholder="FechaFin" class="form-control" title="Fecha Fin">Fecha Fin</span>
        </div>
        <div class="col-sm-1">
            <span id="RevisionPro" name="RevisionPro" class="form-control" title="Revision">Rev</span>
        </div>
        <div class="col-sm-2">
            <span id="CodigoProyecto" placeholder="CodigoProyecto" class="form-control" title="Codigo Proyecto">Codigo Proyecto</span>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <br />
    </div>
</div>
<div class="row" id="etapaRow" style="display:none;">
    <div class="form-group">
        <div class="col-sm-1"> <label class="control-label">Etapa</label></div>
        <div class="col-sm-4">
            <div class="input-group">
                <p id="nombreEtapa" name="nombreEtapa" class="form-control" title="Nombre Etapa"> Nombre Etapa </p>
                <span class="input-group-btn">
                    <button class="btn btn-secondary" type="button" id="btnBuscarEtapa" name="btnBuscarEtapa"><span class="fa fa-search"></span></button>
                </span>
            </div>
            <input type="hidden" id="idEtapaSelect" name="idEtapaSelect" />
        </div>
        <div class="col-sm-2">
            <span id="FechaInicioEtapa" class="form-control" title="Fecha Inicio">Fecha Inicio</span>
        </div>
        <div class="col-sm-2">
            <span id="FechaFinEtapa" class="form-control" title="Fecha Fin">Fecha Fin</span>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <br />
    </div>
</div>
<div class="row" id="planosMontajeRow" style="display:none;">
    <div class="col-sm-1"> <label class="control-label">Montaje</label></div>
    <div class="form-group">
        <div class="col-sm-4">
            <div class="input-group">
                <p id="nombrePlanosMontaje" name="nombrePlanosMontaje" class="form-control" title="Nombre Planos de Montaje"> Nombre Planos de Montaje </p>
                <span class="input-group-btn">
                    <button class="btn btn-secondary" type="button" id="btnBuscarPlanosMontaje" name="btnBuscarPlanosMontaje"><span class="fa fa-search"></span></button>
                </span>
            </div>
            <input type="hidden" id="idPlanoMontajeSelect" name="idPlanoMontajeSelect" />
        </div>
        <div class="col-sm-2">
            <span id="FechaInicioPlanoMontaje" class="form-control" title="Fecha Inicio">Fecha Inicio</span>
        </div>
        <div class="col-sm-2">
            <span id="FechaFinPlanosMontaje" class="form-control" title="Fecha Fin">Fecha Fin</span>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <br />
    </div>
</div>
<div class="row" id="planosDespieceRow" style="display:none;">
    <div class="form-group">
        <div class="col-sm-1"> <label class="control-label">Despiece</label></div>
        <div class="col-sm-4">
            <div class="input-group">
                <p id="nombrePlanosDespiece" name="nombrePlanosDespiece" class="form-control" title="Nombre Planos Despiece"> Nombre Planos Despiece </p>
                <span class="input-group-btn">
                    <button class="btn btn-secondary" type="button" id="btnBuscarPlanosDespiece" name="btnBuscarPlanosDespiece"><span class="fa fa-search"></span></button>
                </span>
            </div>
            <input type="hidden" id="idPlanoDespieceSelect" name="idPlanoDespieceSelect" />
        </div>
        <div class="col-sm-2">
            <span id="CodigoPlanoDespiece" class="form-control" title="Codigo">Codigo</span>
        </div>
        <div class="col-sm-2">
            <span id="NombreTipoConstruccion" class="form-control" title="Tipo Construccion">Tipo Construccion</span>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <br />
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div id="bbGrid-subMarcas" class="col-lg-12">
            <div id="cargandoInfo" style="display:none;">Cargando informacion...</div>
        </div>
    </div>
</div>

<div class="modal fade bs-example-modal-lg" id="nuevo-subMarcas"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevosubMarcasLabel">
</div>

<div class="modal fade bs-example-modal-lg" id="actualiza-subMarcas"
     tabindex="-1" role="dialog"
     aria-labelledby="actulizasubMarcasLabel">
</div>

<div class="modal fade bs-example-modal-lg" id="buscar-General"
     tabindex="-1" role="dialog"
     aria-labelledby="buscarGeneral">
</div>


@section Scripts {
    <script src="~/Scripts/SubMarcas.js"></script>
}

