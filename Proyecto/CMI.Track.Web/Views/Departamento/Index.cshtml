@{
    ViewBag.Title = "Departamentos";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            <small>Lista</small> Departamentos
            <button class="btn btn-outline btn-primary pull-right btnNuevo" style="display: none;">Nuevo</button>
        </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <div class="col-lg-12">
        <div id="bbGrid-clear" class="col-lg-12">  </div>
    </div>
</div>

<div class="modal fade" id="nuevo-departamento"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoDepartamentoLabel">
</div>

<div class="modal fade" id="actualiza-departamento"
     tabindex="-1" role="dialog"
     aria-labelledby="actualizaUsuarioLabel">
</div>


@section Scripts {
    <script src="~/Scripts/Departamento.js"></script>
}



